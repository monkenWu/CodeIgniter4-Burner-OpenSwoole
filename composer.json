{
    "name": "monken/codeigniter4-burner-openswoole",
    "description": "CodeIgniter4 out-of-the-box high-performance server, OpenSwoole Driver.",
    "keywords": [
		"codeigniter",
		"codeigniter4",
		"OpenSwoole",
		"swoole",
		"high-performance",
		"server",
		"burner"
	],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MonkenWu",
            "homepage": "https://github.com/monkenWu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "codeigniter4/framework": "^4",
        "imefisto/psr-swoole-native": "^1.1"
    },
    "autoload": {
		"psr-4": {
			"Monken\\CIBurner\\OpenSwoole": "src/"
		}
	},
    "require-dev": {
        "codeigniter/coding-standard": "^1.4",
        "rector/rector": "0.12.16",
        "openswoole/ide-helper": "@dev"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
